<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"       xmlns:mvc="http://www.springframework.org/schema/context" xmlns:duubo="http://code.alibabatech.com/schema/dubbo"       xmlns:context="http://www.springframework.org/schema/context"       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">    <context:component-scan base-package="test.liuzehua.provider.serviceimpl"/>    <!--定义应用名-->    <dubbo:application name="dubbo-provider"/>    <!--定义暴露的端口-->    <duubo:protocol port="12001"/>    <!--定义注册中心地址-->    <dubbo:registry protocol="zookeeper" address="192.168.81.166:2181"/>    <!--定义发布的类-->    <bean id="userService" class="test.liuzehua.provider.serviceimpl.UserServiceImpl"/>    <!--定义发布到那个接口-->    <dubbo:service interface="test.liuzehua.service.UserService" ref="userService"/></beans>